{
  "productName": "Claude Session Manager",
  "version": "0.1.0",
  "identifier": "com.cloudcode.sessionmanager",
  "build": {
    "beforeDevCommand": "npm run dev:ui",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build:ui",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Claude Session Manager",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
